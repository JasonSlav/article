generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int       @id @default(autoincrement())
  createdAt DateTime? @default(now())
  updatedAt DateTime? @default(now()) @updatedAt
  name      String
  username  String    @unique
  email     String    @unique
  picture   String?
  bio       String?
}
